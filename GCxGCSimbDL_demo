                'A2D4':[float(a2d4.value if a2d4.value else 0),
                        float(a2d4.value if a2d4.value else 0)]
                    }        
        
        matrix_1D = pd.DataFrame(matrix_1D,index=labels)
        matrix_2D = pd.DataFrame(matrix_2D,index=labels)
        
        
